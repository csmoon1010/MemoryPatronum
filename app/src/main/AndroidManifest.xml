<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="techtown.org.memorypatronum">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".RegistrationActivity"/>
        <activity android:name=".ExerciseActivity"/>
        <activity android:name=".ExerciseActivity2"/>
        <activity android:name=".ExPracticeActivity"/>
        <activity android:name=".ExTestActivity"/>
        <activity android:name=".DiaryActivity"/>
        <activity android:name=".DWriteActivity"/>
        <activity android:name=".DWriteActivity2"/>
        <activity android:name=".DMemoryActivity"/>
        <activity android:name=".DMemoryActivity2"/>
        <activity android:name=".DMemoryActivity3"/>
        <activity android:name=".DTestActivity"/>
        <service android:name=".RandomService"></service>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-feature android:name="android.hardware.Camera" />
</manifest>